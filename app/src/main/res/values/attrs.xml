<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="title" format="string" />
    <attr name="leadingIcon" format="reference" />
    <attr name="viewStrokeColor" format="color" />
    <attr name="titleTextColor" format="color" />
    <attr name="tileColor" format="color" />
    <attr name="iconTint" format="color" />


    <declare-styleable name="ButtonView">
        <attr name="text" format="string" />
        <attr name="colorType" format="reference" />
        <attr name="background" format="reference" />
        <attr name="backgroundColor" format="color" />
        <attr name="viewStrokeColor" />
        <attr name="titleTextColor" />


    </declare-styleable>


    <declare-styleable name="ListTile">
        <attr name="title" />
        <attr name="leadingIcon" />
        <attr name="viewStrokeColor" />
        <attr name="titleTextColor" />
        <attr name="tileColor" />
        <attr name="iconTint" />
        <attr name="trailingIcon" format="reference" />

    </declare-styleable>

    <declare-styleable name="ButtonViewAdd">
        <attr name="title" />
        <attr name="leadingIcon" />
        <attr name="viewStrokeColor" />
        <attr name="titleTextColor" />
        <attr name="tileColor" />
        <attr name="iconTint" />
    </declare-styleable>
</resources>